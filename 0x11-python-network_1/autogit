#!/bin/bash

# Loop for 25 times
for ((i=1; i<=25; i++))
do
    # Add a line to the README.md file
    echo "Updated at $(date)" >> README.md

    # Add all changes
    git add .

    # Commit changes
    git commit -m "updated"

    # Push changes
    git push

    # Wait for 15 seconds
    sleep 15
done
